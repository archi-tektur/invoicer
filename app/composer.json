{
    "name": "archi-tektur/invoicer",
    "version": "0.1.1",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/assert": "^3.3",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/console": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^6.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/proxy-manager-bridge": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/uid": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.6"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\UnitTests\\": "tests/unit/",
            "App\\FunctionalTests\\": "tests/functional/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "build": [
            "echo 'Building...'"
        ],
        "vulnerability-scan": [
            "echo 'Scanning vulnerabilities...'"
        ],
        "format-check": [
            "echo 'Checking code format...'"
        ],
        "lint-check": [
            "echo 'Checking code quality...'"
        ],
        "unit-tests": [
            "vendor/bin/phpunit --testsuite 'Unit Tests Suite'"
        ],
        "http-api-tests": [
            "vendor/bin/phpunit --testsuite 'HTTP API Tests Suite'"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.0"
        }
    }
}
